/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in = a, out = Nota);
    Not(in = b, out = Notb);
    And(a = a, b = Notb, out = Andout);
    And(a = Nota, b = b, out = NotAndout);
    Or(a = Andout, b = NotAndout, out = out); 
}
